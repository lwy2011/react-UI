{
    "name": "yongr-ui",
    "version": "0.0.8",
    "description": "react前端UI组件",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index",
    "files": [
        "/dist/**/*"
    ],
    "scripts": {
        "doc": "cross-env NODE_ENV=production webpack --config webpack.config.example.js",
        "test": "cross-env NODE_ENV=test jest --config=jest.config.js --runInBand",
        "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
        "build": "cross-env NODE_ENV=production webpack --config webpack.config.pro.js",
        "ci": "cross-env NODE_ENV=test JEST_JUNIT_OUTPUT=./test-results/jest/results.xml jest --config=jest.config.ci.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/liuwanyong2017/rUI.git"
    },
    "keywords": [
        "react",
        "UI",
        "刘万永"
    ],
    "author": "liuwanyong",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/liuwanyong2017/rUI/issues"
    },
    "homepage": "https://github.com/liuwanyong2017/rUI#readme",
    "devDependencies": {
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-react": "^7.0.0",
        "@types/jest": "^24.0.13",
        "@types/react": "^16.8.17",
        "@types/react-dom": "^16.8.4",
        "@types/react-router": "^5.0.1",
        "@types/react-router-dom": "^4.3.3",
        "@types/react-test-renderer": "^16.8.1",
        "awesome-typescript-loader": "^5.2.1",
        "babel-jest": "^24.8.0",
        "babel-preset-react-app": "^9.0.0",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.1",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.13.0",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.8.0",
        "jest-junit": "^6.4.0",
        "node-sass": "^4.12.0",
        "prism-react-renderer": "^0.1.6",
        "raw-loader": "^2.0.0",
        "react-test-renderer": "^16.8.6",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "svg-sprite-loader": "^4.1.6",
        "ts-jest": "^24.0.2",
        "typescript": "^3.4.5",
        "webpack": "^4.31.0",
        "webpack-cli": "^3.3.2",
        "webpack-dev-server": "^3.3.1"
    },
    "dependencies": {
        "env": "^0.0.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router": "^5.0.0",
        "react-router-dom": "^5.0.0"
